			"url": "http://www.apache.org/licenses/LICENSE-2.0.html"
		},
		"version": "0.9"
	},
	"paths": {
		"/project": {
			"get": {
				"description": "Получить список проектов, доступных данному пользователю",
				"produces": [
					"application/json"
				],
				"responses": {
					"200": {
						"description": "Список проектов",
						"schema": {
							"type": "array",
							"items": {
                "$ref": "#/definitions/Project"
              }
						}
					}
				},
        "security": {
          "api_key": []
        }
			}
		},
		"/project/{id}": {
			"get": {
				"description": "Возвращает проект по ID",
				"summary": "Поиск проекта по ID",
				"operationId": "getProjectsById",
				"produces": [
					"application/json"
				],
				"responses": {
					"200": {
						"description": "Описание проекта",
						"schema": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/Project"
							}
						}
					}
				}
			},
			"parameters": [
				{
					"name": "id",
					"in": "formData",
					"description": "ID проекта",
					"required": true,
					"type": "array",
					"items": {
						"type": "string"
					},
					"collectionFormat": "csv"
				}
			]
		}
	},
  "definitions": {
    "Project": {
      "properties": {
        "id": {"type": "integer", "format": "int64"},
        "name": {"type": "string", "example": "Мой проект 1"},
        "cnt_letter": {"type": "integer", "format": "int64"},
        "cnt_subscriber": {"type": "integer", "format": "int64"},
        "cnt_delivery": {"type": "integer", "format": "int64"}
      },
      "required": ["id", "name"],
      "type": "object"
    }
  },
  "securityDefinitions": {
    "api_key": {
      "in": "query",
      "name": "token",
      "type": "apiKey"
    }
  }


}