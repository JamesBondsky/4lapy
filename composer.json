{
  "name": "adv/4lapy",
  "description": "eshop 4lapy powered by ADV/web-engineering co.",
  "type": "project",
  "authors": [
    {
      "name": "Gripinskiy Sergey",
      "email": "gripinskiy@adv.ru"
    },
    {
      "name": "Panychev Dmitry",
      "email": "panychev@adv.ru"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "vendor-dir": "local/php_interface/vendor"
  },
  "autoload": {
    "psr-4": {
      "Adv\\": "local/php_interface/lib",
      "Adv\\Test\\": "local/php_interface/test"
    }
  },
  "extra": {
    "installer-paths": {
      "local/modules/{$name}/": [
        "type:bitrix-module"
      ]
    }
  },
  "require": {
    "andreyryabin/sprint.migration": "^2.3",
    "symfony/routing": "~3.2.8",
    "symfony/console": "~3.2.8",
    "symfony/finder": "~3.2.8",
    "monolog/monolog": "^1.22",
    "symfony/dependency-injection": "~3.2.8",
    "symfony/config": "~3.2.8",
    "symfony/yaml": "~3.2.8",
    "symfony/cache": "~3.2.8",
    "symfony/http-foundation": "~3.2.8",
    "symfony/event-dispatcher": "~3.2.8",
    "symfony/http-kernel": "~3.2.8",
    "symfony/expression-language": "~3.2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  }
}
